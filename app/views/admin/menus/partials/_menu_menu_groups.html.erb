    <div class="dd" id="nestable">
	    <ol class='sortable connect dd-list' data-menuid='<%= @menu.id %>'>
	    	<%= menu_list_loop @menu.id %>
	    </ol>
    </div>
	<hr>
	<div class="pull-left">
		<small>key:- <%= @menu.key %></small>
	</div>
	<div class='btn-group pull-right'>
		<a class='btn btn-sm btn-default' href='/admin/menus/<%= @menu.id %>/edit'>
			<i class='fa fa-pencil'></i>&nbsp;edit
		</a>
		<a rel='nofollow' data-method='delete' data-confirm='Are you sure?' class='btn btn-sm btn-danger' href='/admin/menus/<%= @menu.id %>'>
			<i class='fa fa-trash-o'></i>&nbsp;delete
		</a>
	</div>

	<div class='clearfix'></div>
